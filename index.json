{
  "Api": {
  "author": "FalsisDev",
  "description": "My personal api",
  "language": ["en-US", "en", "tr"],
  "Npm": "/npm?name=:name&key=YOUR_KEY",
  "Owofy": "/owofy?message=:name",
  "Cat": "/cat",
  "Dog": "/dog",
  "Lyrics": "/lyrics?title=:name",
  "Translate": "/translate?text=Hello&lang=tr&from=en&key=YOUR_KEY",
  "Wikipedia": "/wiki?wiki=:name",
  "Color": "/color?hex=:hex || /color?rgb=:rgb"
  },
  "key": "Cannot find query key",
    "wiki":{
   "error": "Cannot find query wiki.",
   "queries":["wiki", "lang"]
  },

    "color":{
   "error": "Cannot find query hex.",
   "error2": "Cannot find query rgb",
   "queries":["hex", "rgb"]
  },

    "translate":{
   "error": "Cannot find query text.",
   "error2": "Cannot find query from",
   "error3": "Cannot find query lang",
   "queries":["text", "from", "lang", "key"]
  },
  "npm":{
    "error": "Cannot find query name",
    "error2": "No such npm found",
    "queries": ["name", "key"]
  },
  "owofy":{
    "error": "Cannot find query message",
    "queries": ["message"]
  },
  "lyrics":{
    "error": "Cannot find query title",
    "queries": ["title"]
  }

}
